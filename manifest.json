{
  "name": "Search image with Google Lens",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "An extension to search image with Google Lens on context menu.",
  "icons": {
    "16": "icons/google_lens_16x16.png",
    "32": "icons/google_lens_32x32.png",
    "48": "icons/google_lens_48x48.png",
    "64": "icons/google_lens_64x64.png",
    "128": "icons/google_lens_128x128.png"
  },
  "permissions": ["contextMenus"],
  "background": {
    "service_worker": "background.js"
  }
}
